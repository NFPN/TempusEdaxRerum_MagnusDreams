<Window x:Class="MagnusDreams.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        WindowStartupLocation="CenterScreen"
        Title="MainWindow" Height="720" Width="1280" WindowStyle="None">
    
    <Grid x:Name="GridMenu" Background="#FF818181">
        <Image x:Name="Fundo" HorizontalAlignment="Left" Height="728" VerticalAlignment="Top" Width="1280" Source="Images/Frame.png" Stretch="Fill" Margin="0,0,0,-8"/>
        <!-- Content control pra controlar qual usercontrol aparece em main-->
        <ContentControl x:Name="contentControl" Grid.Row="1"/>

    </Grid>
</Window>
